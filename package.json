{
	"name": "@prismicio/svelte",
	"version": "0.0.6",
	"description": "Svelte plugin, components, and composables to fetch and present Prismic content",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"main": "index.js",
	"module": "index.js",
	"types": "index.d.ts",
	"svelte": "./index.js",
	"scripts": {
		"build": "svelte-package package && shx cp -r src dist/src",
		"dev": "vite dev",
		"format": "prettier --write .",
		"prepare": "npm run build",
		"release": "npm run build && npm run test && standard-version && git push --follow-tags && npm run build && npm publish ./dist",
		"release:dry": "standard-version --dry-run",
		"release:alpha": "npm run build && npm run test && standard-version --release-as major --prerelease alpha && git push --follow-tags && npm run build && npm publish ./dist --tag alpha",
		"release:alpha:dry": "standard-version --release-as major --prerelease alpha --dry-run",
		"lint": "eslint --ext .js,.ts .",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"test": "npm run lint && npm run unit",
		"preview": "svelte-kit preview",
		"package": "svelte-package package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
		"@prismicio/client": "^6.7.3",
		"@prismicio/helpers": "^2.3.9",
		"@sveltejs/kit": "^1.8.3",
		"svelte": "^3.55.1"
	},
	"devDependencies": {
		"@prismicio/mock": "^0.2.0",
		"@prismicio/types": "^0.2.7",
		"@rollup/plugin-inject": "^5.0.3",
		"@sveltejs/package": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"@vitest/coverage-c8": "^0.28.5",
		"c8": "^7.13.0",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"prettier": "^2.8.4",
		"prettier-plugin-jsdoc": "^0.3.38",
		"prettier-plugin-svelte": "^2.9.0",
		"shx": "^0.3.4",
		"standard-version": "^9.3.2",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"svelte2tsx": "^0.6.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vitest": "^0.28.5"
	},
	"engines": {
		"node": ">=12.7.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"optionalDependencies": {
		"@esbuild/android-arm": "^0.17.10"
	}
}
